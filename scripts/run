#!/bin/bash

source data/config

case $fileExt in
	sh)
			/bin/bash $file
		;;
    c)
        if gcc $file -o $tempFile; then
        	./$tempFile
        	rm $tempFile
        fi
        ;;
    cpp)
        if g++ $file -o $tempFile; then
        	./$tempFile
        	rm $tempFile
        fi
        ;;
    py)
        python $file
        ;;
    js)
        node $file
        ;;
    *)
    	if [[ -x FILE ]]; then
    		$file
    	else
        	echo "$fileExt extension not defined"
        	exit 1
        fi
        ;;
esac
