#!/bin/bash

case expression in  
    c)
        if gcc $file -o $tempFile; then
        	./$tempFile
        	rm $tempFile
        fi
        ;;
    cpp)
        if g++ $file -o $tempFile; then
        	./$tempFile
        	rm $tempFile
        fi
        ;;
    pattern_3)  
        statements  
        ;;  
    pattern-n)  
        statements  
        ;;  
    *)  
        statements  
        ;;  
esac
